#################################################################
#    Object Properties
#################################################################

###  urn:absolute:Server#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              owl:inverseOf :isComponentOf ;
              rdfs:domain :Machine ;
              rdfs:range :Component ;
              owl:propertyDisjointWith :isComponentOf .


###  urn:absolute:Server#isComponentOf
:isComponentOf rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  urn:absolute:Server#hasComputePower
:hasComputePower rdf:type owl:DatatypeProperty ,
