###  urn:absolute:Server#hasStorage
:hasStorage rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty .


#################################################################
#    Classes
#################################################################

###  urn:absolute:Server#Component
:Component rdf:type owl:Class ;
           owl:disjointWith :Machine .


###  urn:absolute:Server#ComputeMachine
:ComputeMachine rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Machine
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasComponent ;
                                                             owl:qualifiedCardinality "4"^^xsd:nonNegativeInteger ;
                                                             owl:onClass :Processor
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasComponent ;
                                                             owl:maxQualifiedCardinality "4"^^xsd:nonNegativeInteger ;
                                                             owl:onClass :HDD
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                owl:disjointWith :StorageMachine .


###  urn:absolute:Server#HDD
:HDD rdf:type owl:Class ;
     rdfs:subClassOf :Component .


###  urn:absolute:Server#HighPower
:HighPower rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Machine
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasComputePower ;
                                                        owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                             owl:onDatatype xsd:integer ;
                                                                             owl:withRestrictions ( [ xsd:minInclusive 50
                                                                                                    ]
                                                                                                  )
                                                                           ]
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf :Machine .


###  urn:absolute:Server#HighStorage
:HighStorage rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Machine
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasStorage ;
                                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                               owl:onDatatype xsd:integer ;
                                                                               owl:withRestrictions ( [ xsd:minInclusive 50
                                                                                                      ]
                                                                                                    )
                                                                             ]
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :Machine .


###  urn:absolute:Server#Machine
:Machine rdf:type owl:Class ;
         rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasComponent ;
                                                  owl:someValuesFrom :HDD
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasComponent ;
                                                  owl:someValuesFrom :Processor
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasComponent ;
                                                  owl:someValuesFrom :RAM
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] .


###  urn:absolute:Server#Processor
:Processor rdf:type owl:Class ;
           rdfs:subClassOf :Component .


###  urn:absolute:Server#RAM
:RAM rdf:type owl:Class ;
     rdfs:subClassOf :Component .


###  urn:absolute:Server#StorageMachine
:StorageMachine rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Machine
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasComponent ;
                                                             owl:minQualifiedCardinality "24"^^xsd:nonNegativeInteger ;
                                                             owl:onClass :HDD
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasComponent ;
                                                             owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                             owl:onClass :Processor
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasComponent ;
                                                             owl:maxQualifiedCardinality "60"^^xsd:nonNegativeInteger ;
                                                             owl:onClass :HDD
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :Machine .


